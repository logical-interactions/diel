{
  "name": "diel",
  "version": "1.0.2",
  "description": "data interaction events log is a new SQL embeded DSL for managing the state of history-aware interactive applications",
  "bin": {
    "diel-cli": "lang-dist/lib/compile.js"
  },
  "files": [
    "dist"
  ],
  "main": "lang-dist/lib/index.js",
  "types": "lang-dist/lib/index.d.ts",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --port 8080 --output-public-path=/UI-dist/",
    "build": "tsc && cp src/compiler/static.sql dist/compiler",
    "testCli": "ts-node --no-cache src/lib/compile.ts",
    "testCliOnline": "node --inspect-brk -r ts-node/register src/lib/compile.ts",
    "lang": "antlr4ts -visitor -no-listener src/parser/grammar/DIEL.g4",
    "test": "ts-node --no-cache src/tests/test.ts",
    "probe": "ts-node --no-cache src/tests/debugTool/probeTest.ts",
    "debug": "node --inspect-brk -r ts-node/register src/tests/test.ts",
    "token": "cp src/parser/grammar/DIEL.g4 src/debug/ && cd src/debug && echo ./antlr4-tester.sh DIEL.g4 \"<your query>\" queries"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yifanwu/diel.git"
  },
  "keywords": [
    "interaction",
    "history",
    "sql"
  ],
  "author": "yifan wu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yifanwu/diel/issues"
  },
  "homepage": "https://github.com/yifanwu/diel#readme",
  "devDependencies": {
    "antlr4ts-cli": "^0.4.0-alpha.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3",
    "tslint": "^5.11.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "source-map-loader": "^0.2.3",
    "tslint-loader": "^3.5.3",
    "awesome-typescript-loader": "^3.4.1",
    "typescript-formatter": "^7.2.2"
  },
  "dependencies": {
    "@types/d3": "^4.12.0",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "d3": "^4.12.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "@types/glob": "^7.1.1",
    "@types/sql.js": "0.0.29",
    "antlr4ts": "^0.4.1-alpha.0",
    "commander": "^2.19.0",
    "glob": "^7.1.3",
    "sql-formatter": "^2.3.2",
    "sql.js": "^0.5.0"
  }
}
